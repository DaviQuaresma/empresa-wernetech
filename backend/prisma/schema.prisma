generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phone     String
  company   String?
  position  String?
  segment   String?
  message   String
  createdAt DateTime @default(now())
}
